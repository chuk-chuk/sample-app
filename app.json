{
  "expo": {
    "name": "sample-app",
    "description": "Sample app",
    "slug": "sample-app",
    "sdkVersion": "39.0.0",
    "version": "0.2.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "backgroundColor": "#bdacc2",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "translucent": false,
      "barStyle": "light-content",
      "backgroundColor": "#a834eb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "uk.co.simple.app",
      "buildNumber": "1"
    },
    "android": {
      "package": "uk.co.simple.app",
      "versionCode": 1
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7e52ac"
    }
  }
}
